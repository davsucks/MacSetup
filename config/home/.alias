alias a='alias'

a ..='cd ../'
a ...='cd ../../'
a ..3='cd ../../../'
a ..4='cd ../../../../'
a ..5='cd ../../../../../'
a ..6='cd ../../../../../../'
a cp='cp -iv'
a dockerClean='docker rm $(docker ps -a -q); docker rmi $(docker images -q)'
a dh='du -h -d'
a gw='./gradlew'
a less='less -FSRXc'
a ll='ls -FGlAahp'
a mkdir='mkdir -pv'
a mv='mv -iv'
a myip='ipconfig getifaddr en0'
a realias='source ~/.alias'
a where='networksetup -getcurrentlocation'

mcd () { mkdir $1 && cd $1; }

